# frontend/.env
# Environnement
VITE_APP_NAME="ByGagoos Ink"
VITE_APP_VERSION="1.0.0"
VITE_APP_ENV="production"
VITE_BUILD_DATE="2024-01-16"

# API Configuration
VITE_API_URL="https://bygagoos-ink-backend.vercel.app/api"
VITE_API_TIMEOUT="30000"
VITE_API_RETRY_ATTEMPTS="3"

# PWA Configuration
VITE_PWA_NAME="ByGagoos Ink"
VITE_PWA_SHORT_NAME="ByGagoos"
VITE_PWA_THEME_COLOR="#1e293b"
VITE_PWA_BACKGROUND_COLOR="#f8fafc"

# Analytics (optionnel)
VITE_GOOGLE_ANALYTICS_ID=""
VITE_SENTRY_DSN=""

# Features Flags
VITE_ENABLE_PWA="true"
VITE_ENABLE_ANALYTICS="false"
VITE_ENABLE_SENTRY="false"
VITE_ENABLE_OFFLINE="true"

# Development
VITE_LOCAL_DEV="false"
VITE_DEBUG_MODE="false"
VITE_SHOW_DEV_TOOLS="false"