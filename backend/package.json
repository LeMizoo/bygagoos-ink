{
  "name": "bygagoos-ink-backend",
  "version": "1.0.0",
  "description": "Backend pour ByGagoos-Ink",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test:db": "node test-db.js",
    "prisma:seed": "dotenv -e .env -- node prisma/seed-simple.js",
    "prisma:studio": "dotenv -e .env -- npx prisma studio",
    "prisma:generate": "dotenv -e .env -- npx prisma generate",
    "prisma:push": "dotenv -e .env -- npx prisma db push",
    "prisma:migrate": "dotenv -e .env -- npx prisma migrate dev"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "bcryptjs": "^3.0.3",
    "jsonwebtoken": "^9.0.3",
    "dotenv": "^16.3.1",
    "@prisma/client": "^5.7.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "prisma": "^5.7.0",
    "pg": "^8.11.3",
    "dotenv-cli": "^7.3.0"
  },
  "keywords": ["bygagoos", "family", "business"],
  "author": "BYGAGOOS FAMILY",
  "license": "MIT"
}